#include "uart_communication.h"

enum CommuanicationState{Wait_RST, Send_ADC, Wait_ADC};
enum CommuanicationState commu_state = Wait_RST;

uart_communication_fsm(){

	switch (commu_state){
	case Wait_RST:
		break;
	case Send_ADC:
		break;
	case Wait_ADC:
		break;

	}
}
